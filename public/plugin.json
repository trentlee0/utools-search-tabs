{
  "pluginName": "搜索标签页",
  "author": "Trent0",
  "homepage": "https://github.com/trentlee0",
  "description": "搜索浏览器标签页，支持 Safari、Edge、Chrome",
  "version": "1.0.0",
  "logo": "icon/logo.png",
  "platform": [
    "darwin"
  ],
  "preload": "preload.js",
  "features": [
    {
      "code": "search-safari-tab",
      "icon": "icon/safari.png",
      "explain": "搜索 Safari 中打开的标签页",
      "cmds": [
        "Safari Search Tabs",
        "Safari 搜索标签页",
        {
          "type": "window",
          "label": "搜索标签页",
          "match": {
            "app": [
              "Safari.app"
            ]
          }
        }
      ]
    },
    {
      "code": "search-edge-tab",
      "icon": "icon/edge.png",
      "explain": "搜索 Edge 中打开的标签页",
      "cmds": [
        "Edge Search Tabs",
        "Edge 搜索标签页",
        {
          "type": "window",
          "label": "搜索标签页",
          "match": {
            "app": [
              "Microsoft Edge.app"
            ]
          }
        }
      ]
    },
    {
      "code": "search-chrome-tab",
      "icon": "icon/chrome.png",
      "explain": "搜索 Chrome 中打开的标签页",
      "cmds": [
        "Chrome Search Tabs",
        "Chrome 搜索标签页",
        {
          "type": "window",
          "label": "搜索标签页",
          "match": {
            "app": [
              "Google Chrome.app"
            ]
          }
        }
      ]
    },
    {
      "code": "search-vivaldi-tab",
      "icon": "icon/vivaldi.png",
      "explain": "搜索 Vivaldi 中打开的标签页",
      "cmds": [
        "Vivaldi Search Tabs",
        "Vivaldi 搜索标签页",
        {
          "type": "window",
          "label": "搜索标签页",
          "match": {
            "app": [
              "Vivaldi.app"
            ]
          }
        }
      ]
    },
    {
      "code": "open-safari",
      "icon": "icon/safari.png",
      "explain": "在 Safari 中打开当前页面",
      "cmds": [
        {
          "type": "window",
          "label": "在 Safari 中打开当前页面",
          "match": {
            "app": [
              "Microsoft Edge.app",
              "Google Chrome.app",
              "Vivaldi.app"
            ]
          }
        }
      ]
    },
    {
      "code": "open-edge",
      "icon": "icon/edge.png",
      "explain": "在 Edge 中打开当前页面",
      "cmds": [
        {
          "type": "window",
          "label": "在 Edge 中打开当前页面",
          "match": {
            "app": [
              "Safari.app",
              "Google Chrome.app",
              "Vivaldi.app"
            ]
          }
        }
      ]
    },
    {
      "code": "open-chrome",
      "icon": "icon/chrome.png",
      "explain": "在 Chrome 中打开当前页面",
      "cmds": [
        {
          "type": "window",
          "label": "在 Chrome 中打开当前页面",
          "match": {
            "app": [
              "Safari.app",
              "Microsoft Edge.app",
              "Vivaldi.app"
            ]
          }
        }
      ]
    },
    {
      "code": "open-vivaldi",
      "icon": "icon/vivaldi.png",
      "explain": "在 Vivaldi 中打开当前页面",
      "cmds": [
        {
          "type": "window",
          "label": "在 Vivaldi 中打开当前页面",
          "match": {
            "app": [
              "Safari.app",
              "Microsoft Edge.app",
              "Google Chrome.app"
            ]
          }
        }
      ]
    },
    {
      "code": "new-safari",
      "icon": "icon/safari.png",
      "explain": "在 Safari 中新建标签页",
      "cmds": [
        "Safari New Tab",
        "Safari 新建标签页",
        {
          "type": "window",
          "label": "新建标签页",
          "match": {
            "app": [
              "Safari.app"
            ]
          }
        }
      ]
    },
    {
      "code": "new-edge",
      "icon": "icon/edge.png",
      "explain": "在 Edge 中新建标签页",
      "cmds": [
        "Edge New Tab",
        "Edge 新建标签页",
        {
          "type": "window",
          "label": "新建标签页",
          "match": {
            "app": [
              "Microsoft Edge.app"
            ]
          }
        }
      ]
    },
    {
      "code": "new-chrome",
      "icon": "icon/chrome.png",
      "explain": "在 Chrome 中新建标签页",
      "cmds": [
        "Chrome New Tab",
        "Chrome 新建标签页",
        {
          "type": "window",
          "label": "新建标签页",
          "match": {
            "app": [
              "Google Chrome.app"
            ]
          }
        }
      ]
    },
    {
      "code": "new-vivaldi",
      "icon": "icon/vivaldi.png",
      "explain": "在 Vivaldi 中新建标签页",
      "cmds": [
        "Vivaldi New Tab",
        "Vivaldi 新建标签页",
        {
          "type": "window",
          "label": "新建标签页",
          "match": {
            "app": [
              "Vivaldi.app"
            ]
          }
        }
      ]
    }
  ]
}